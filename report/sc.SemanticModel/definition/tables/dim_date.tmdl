	partition dim_date = calculated
		mode: import
		source = ```
				
				VAR BaseCalendar =
				    CALENDARAUTO(12)
				RETURN
				ADDCOLUMNS(
				    BaseCalendar,
				
				    -- Surrogate Date Key (YYYYMMDD)
				    "date_key", VALUE(FORMAT([Date], "YYYYMMDD")),
				
				    -- Year / Quarter
				    "year", YEAR([Date]),
				    "year_short", RIGHT(YEAR([Date]), 2),
				    "quarter", "Q" & FORMAT([Date], "Q"),
				    "year_quarter", YEAR([Date]) & " Q" & FORMAT([Date], "Q"),
				
				    -- Month
				    "month_number", MONTH([Date]),
				    "month_name", FORMAT([Date], "MMMM"),
				    "month_short", FORMAT([Date], "MMM"),
				    "year_month", FORMAT([Date], "YYYY-MM"),
				    "year_month_sort", YEAR([Date]) * 100 + MONTH([Date]),
				
				    -- Week
				    "week_number", WEEKNUM([Date], 2),
				    "iso_week", WEEKNUM([Date], 21),
				    "year_week", YEAR([Date]) & "-W" & FORMAT(WEEKNUM([Date], 21), "00"),
				
				    -- Day
				    "day_of_month", DAY([Date]),
				    "day_of_week_number", WEEKDAY([Date], 2),
				    "day_of_week_name", FORMAT([Date], "dddd"),
				    "day_of_week_short", FORMAT([Date], "ddd"),
				
				    -- Flags
				    "is_weekend", IF(WEEKDAY([Date], 2) >= 6, TRUE(), FALSE()),
				    "is_current_year", YEAR([Date]) = YEAR(TODAY()),
				    "is_current_month", FORMAT([Date], "YYYYMM") = FORMAT(TODAY(), "YYYYMM"),
				    "is_current_date", [Date] = TODAY(),
				
				    -- Month boundaries
				    "month_start_date", DATE(YEAR([Date]), MONTH([Date]), 1),
				    "month_end_date", EOMONTH([Date], 0),
				
				    -- Year boundaries
				    "year_start_date", DATE(YEAR([Date]), 1, 1),
				    "year_end_date", DATE(YEAR([Date]), 12, 31)
				)
				
				```

